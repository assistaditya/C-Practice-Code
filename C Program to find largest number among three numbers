#include<stdio.h>
#include<conio.h>

int main()
{
  
    int a,b,c;
    int largest;
    printf("\n Enter first number = ");
    scanf("%d",&a);
    printf("\n Enter second number = ");
    scanf("%d",&b);
    printf("\n Enter third number = ");
    scanf("%d",&c);
    
    if(a>b && a>c)
    largest=a;
    else if(b>a && b>c)
    largest=b;
    else
    largest=c;
    
    printf("\n largest number = %d ",largest);
    return 0;
    
    
}
