#include<stdio.h>
#include<conio.h>

int main()

{
    char ch;
    printf("\n Enter any character to check =  ");
    scanf("%c",&ch);
    
    switch(ch)
    
    {
        case 'a':
         printf("\n Enter character is %c vowel",ch);
         break;
        
        case 'e':
         printf("\n Enter character is %c vowel.",ch);
         break;
         
        case 'i':
         printf("\n Enter character is %c vowel.",ch); 
         break;
         
        case 'o':
         printf("\n Enter character is %c vowel.",ch); 
         break;
         
        case 'u':
         printf("\n Enter character is %c vowel.",ch); 
         break;
         
        default:
         printf("\n The character %c is a constant.",ch);
    }
    return 0;
}
