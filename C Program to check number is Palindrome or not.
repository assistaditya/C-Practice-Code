#include<stdio.h>


int main()

{
    int n,r,sum=0,temp;
    printf("\n Enter any number = ");
    scanf("%d",&n);
    temp=n;
    
    while(n>0)
    {
        r=n%10;
        sum=r+(sum*10);
        n=n/10;
    }
    if(temp==sum)
    {
        printf("\n Palindrome Number.");
    }     
    else
    {
    printf("\n Not Palindrome Number.");
    }
    printf("\n Aditya Panditrao.");
    return 0;
}
