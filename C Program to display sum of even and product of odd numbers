#include<stdio.h>
#include<conio.h>

int main()

{
    int n,sum=0,product=1,i;
    printf("\n Enter 10 numbers = ");
    for(i=0;i<10;i++)
    {
        printf("\n Enter the number %d = ",i+1);
        scanf("%d",&n);
        if(n%2==0)
        
        sum=sum+n;
        
        else
        product=product*n;
    }
    printf("\n The sum of even number = %d ",sum);
    printf("\n The product of odd number =%d",product);
    
    return 0;
}
