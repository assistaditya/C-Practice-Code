#include<stdio.h>
#include<conio.h>
#define MAX_SIZE 1000

int main()

{
    int arr[MAX_SIZE];
    int i,n;
    
    printf("\n Enter the size of an array = ");
    scanf("%d",&n);
    printf("\n Enter %d elements  in an array ",n);
    for(i=0;i<n;i++)
    {
        printf("\n Enter elements %d in an array = ",i+1);
        scanf("%d",&arr[i]);
    }
    
    printf("\n elements in an array are ");
    for(i=0;i<n;i++)
    {
        printf("\t %d",arr[i]);
    }
    return 0;
}
